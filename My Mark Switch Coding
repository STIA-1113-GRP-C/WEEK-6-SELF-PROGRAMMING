package mySwitch;
import java.util.Scanner;
public class myMarkSwitch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Scanner sc = new Scanner(System.in);
        int mark;
        
        System.out.print("Enter your mark : ");
        mark = sc.nextInt();
        
        mark = mark / 10;
        
        switch(mark) {// switch hanya guna int dan character
        
        case 10:
        case 9 :
        	System.out.println(" Excellent: Grade A");
        	break;
        case 8 :
        	System.out.println("Good: Grade B");
            break;
        case 7 :
        	System.out.println("Good: Grade C ");
        	break;
        case 6 :
        	System.out.println("Good: Grade D");
        	break;
        case 5 :
        	System.out.println("Good: Grade E");
        	break;
        case 4 :
        	System.out.println("Just Passed: Grade F");
        	break;
        default:
        	System.out.println(" Failed!");
        }
        sc.close();
	}

}
