package mySwitch;
import java.util.Scanner;
public class myGradeSwitch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Scanner sc = new Scanner(System.in);
        
        System.out.print("Enter your grade from A,B,C,D,E and F : ");
        String str = sc.next();
        char grade = str.charAt(0);
        
        switch(grade) {
        
        case 'A':
        	System.out.println("Grade A then  marks >=90");
            break;
        case 'B' :
            System.out.println("Grade B marks >= 80 and less than 90");
            break;
        case 'C' :
            System.out.println("Grade C marks >= 70 and less than 80");
            break;
        case  'D' :
            System.out.println("Grade D marks >= 60 and less than 70");
            break;
        case 'E' :
            System.out.println("Grade E marks >= 50 and less than 60");
            break;
        case 'F' :
            System.out.println("Grade F mark <= 40");
            break;
        default :
        System.out.println("Invalid Grade");
        }
        
	}

}
